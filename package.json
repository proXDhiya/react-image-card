{
  "name": "@proxdhiya/react-image-card",
  "private": false,
  "version": "0.0.6",
  "type": "module",
  "description": "React Image Card",
  "scripts": {
    "dev": "vite --port 3000",
    "dev:host": "vite --host --port 3000",
    "build": "tsc && vite build --mode production",
    "preview": "vite preview",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "lint:fix": "eslint --fix 'src/**/*.{jsx,ts,tsx}'",
    "format": "prettier --write src//**/*.{ts,tsx,css} --config ./.prettierrc",
    "prepare": "husky install",
    "publish": "yarn build && npm publish --access public"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/react-image-card.es.js",
      "require": "./dist/react-image-card.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "lib": {
    "entry": "dist/react-image-card.es.js",
    "formats": [
      "es",
      "umd"
    ],
    "fileName": "react-image-card"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "yarn  eslint",
      "yarn  format"
    ]
  },
  "main": "./dist/react-image-card.umd.js",
  "module": "./dist/react-image-card.es.js",
  "types": "./dist/index.d.ts",
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^18.7.6",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.34.0",
    "@typescript-eslint/parser": "^5.34.0",
    "@vitejs/plugin-react": "^2.1.0",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^4.6.4",
    "vite": "^3.1.0",
    "vite-plugin-dts": "^1.4.1",
    "vite-plugin-linter": "^1.2.0",
    "vite-tsconfig-paths": "^3.5.0"
  },
  "peerDependencies": {
    "react": "16.8.0 || >=17.x",
    "react-dom": "16.8.0 || >=17.x"
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/proXDhiya/react-image-card"
  },
  "keywords": [
    "react",
    "vite",
    "typescript",
    "library",
    "component",
    "ui",
    "image",
    "card"
  ],
  "author": "proXDhiya",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/proXDhiya/react-image-card/issues"
  },
  "homepage": "https://github.com/proXDhiya/react-image-card#readme"
}
